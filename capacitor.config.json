{
	"appId": "ng.gofer.business",
	"appName": "Gofer",
	"webDir": "dist",
	  
  "plugins":{
	"SplashScreen": {
		"launchShowDuration": 0,
		"backgroundColor": "#FFFFFF",
		"launchAutoHide": true,
		"androidSplashResourceName": "launch_splash"
	},

		"Keyboard": {
			"resize": "body",
			"resizeOnFullScreen": false
		},
		
		"Server": {
			"cleartext": true,
			"hostname": "localhost:8100",
			"androidScheme": "http"
			
		}
	}
}

